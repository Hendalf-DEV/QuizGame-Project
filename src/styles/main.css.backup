  gap: 0.7rem;
  background: rgba(255, 255, 255, 0.05);
  border: 2px solid rgba(255, 255, 255, 0.1);
  border-radius: 12px;
  padding: 0.5rem 0.7rem;
  transition: all 0.3s ease;
  animation: slideInLeft 0.6s ease-out;
  position: relative;
  overflow: hidden;
}

.rank-item::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 4px;
  height: 100%;
  background: transparent;
  transition: background 0.3s ease;
}

.rank-item:hover {
  transform: translateX(5px);
  background: rgba(255, 255, 255, 0.08);
  border-color: rgba(255, 255, 255, 0.2);
}

.rank-item:nth-child(1) {
  animation-delay: 0s;
}

.rank-item:nth-child(2) {
  animation-delay: 0.05s;
}

.rank-item:nth-child(3) {
  animation-delay: 0.1s;
}

.rank-item:nth-child(4) {
  animation-delay: 0.15s;
}

.rank-item:nth-child(5) {
  animation-delay: 0.2s;
}

.rank-gold {
  background: linear-gradient(135deg, rgba(255, 215, 0, 0.15), rgba(255, 255, 255, 0.05));
  border-color: rgba(255, 215, 0, 0.4);
}

.rank-gold::before {
  background: linear-gradient(180deg, #FFD700, #FFA500);
}

.rank-silver {
  background: linear-gradient(135deg, rgba(192, 192, 192, 0.15), rgba(255, 255, 255, 0.05));
  border-color: rgba(192, 192, 192, 0.4);
}

.rank-silver::before {
  background: linear-gradient(180deg, #C0C0C0, #A8A8A8);
}

.rank-bronze {
  background: linear-gradient(135deg, rgba(205, 127, 50, 0.15), rgba(255, 255, 255, 0.05));
  border-color: rgba(205, 127, 50, 0.4);
}

.rank-bronze::before {
  background: linear-gradient(180deg, #CD7F32, #B87333);
}

.rank-item.current-user {
  background: linear-gradient(135deg, rgba(67, 233, 123, 0.15), rgba(56, 249, 215, 0.1));
  border-color: rgba(67, 233, 123, 0.5);
  box-shadow: 0 0 20px rgba(67, 233, 123, 0.2);
}

.rank-item.current-user::before {
  background: linear-gradient(180deg, #43e97b, #38f9d7);
}

.rank-position {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  min-width: 50px;
  flex-shrink: 0;
  gap: 0.15rem;
  text-align: center;
}

.rank-position .rank-emoji {
