
@import url('https://fonts.googleapis.com/css2?family=Fredoka+One:wght@400&family=Poppins:wght@300;400;500;600;700&display=swap');

:root {
  --primary-gradient: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  --secondary-gradient: linear-gradient(135deg, #f093fb 0%, #f5576c 100%);
  --success-gradient: linear-gradient(135deg, #4facfe 0%, #00f2fe 100%);
  --warning-gradient: linear-gradient(135deg, #43e97b 0%, #38f9d7 100%);
  --background: linear-gradient(135deg, #667eea 0%, #764ba2 50%, #f093fb 100%);
  --card-bg: rgba(255, 255, 255, 0.1);
  --card-border: rgba(255, 255, 255, 0.2);
  --text-primary: #ffffff;
  --text-secondary: rgba(255, 255, 255, 0.8);
  --shadow: 0 20px 40px rgba(0, 0, 0, 0.1);
  --shadow-hover: 0 30px 60px rgba(0, 0, 0, 0.2);
}

* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
}

html, body {
  width: 100%;
  height: 100%;
  overflow: hidden;
}

body {
  font-family: 'Poppins', sans-serif;
  background: var(--background);
  min-height: 100vh;
  position: relative;
  cursor: url('data:image/svg+xml;base64,') 16 16, auto;
}

button, .nav-card, a, [role="button"] {
  cursor: url('data:image/svg+xml;base64,') 16 16, pointer;
}

body::before {
  content: '';
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: url('data:image/svg+xml,<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100"><circle cx="20" cy="20" r="2" fill="rgba(255,255,255,0.1)"><animate attributeName="opacity" values="0;1;0" dur="3s" repeatCount="indefinite"/></circle><circle cx="80" cy="40" r="1.5" fill="rgba(255,255,255,0.1)"><animate attributeName="opacity" values="0;1;0" dur="4s" repeatCount="indefinite" begin="1s"/></circle><circle cx="40" cy="80" r="1" fill="rgba(255,255,255,0.1)"><animate attributeName="opacity" values="0;1;0" dur="5s" repeatCount="indefinite" begin="2s"/></circle></svg>');
  pointer-events: none;
  z-index: 0;
}

#root {
  position: relative;
  z-index: 1;
}

#funny-h1 {
  font-family: 'Fredoka One', cursive;
  font-size: clamp(3rem, 8vw, 6rem);
  background: linear-gradient(45deg, #fff, #f0f8ff, #e6f3ff, #fff);
  background-size: 400% 400%;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
  text-align: center;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 10;
  text-shadow: 0 0 30px rgba(255, 255, 255, 0.5);
  cursor: pointer;
  user-select: none;
  transition: all 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  animation:
    floatTitle 4s ease-in-out infinite,
    rotateTitle 6s ease-in-out infinite,
    shimmer 3s ease-in-out infinite;
  opacity: 1;
}

#funny-h1.clickable {
  animation:
    floatTitle 4s ease-in-out infinite,
    rotateTitle 6s ease-in-out infinite,
    shimmer 3s ease-in-out infinite,
    pulse 3s ease-in-out infinite;
}

#funny-h1:hover {
  animation:
    bounceHover 0.6s ease-in-out,
    shimmerFast 0.8s ease-in-out infinite;
  filter: drop-shadow(0 0 20px rgba(255, 255, 255, 0.8));
}

#funny-h1:active {
  animation: explode 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  filter: drop-shadow(0 0 40px rgba(255, 255, 255, 1))
          drop-shadow(0 0 80px rgba(102, 126, 234, 0.8));
}

.click-hint {
  position: absolute;
  top: 110%;
  left: 50%;
  transform: translateX(-50%);
  font-family: 'Poppins', sans-serif;
  font-size: clamp(0.9rem, 2vw, 1.1rem);
  font-weight: 400;
  color: rgba(255, 255, 255, 0.8);
  background: rgba(255, 255, 255, 0.1);
  backdrop-filter: blur(10px);
  padding: 0.8rem 1.5rem;
  border-radius: 25px;
  border: 1px solid rgba(255, 255, 255, 0.2);
  animation: hintPulse 2s ease-in-out infinite;
  white-space: nowrap;
}

#funny-h1.auth-mode {
  top: 15%;
  font-size: clamp(2rem, 5vw, 3.5rem);
  animation:
    floatTitleSmall 3s ease-in-out infinite,
    shimmer 3s ease-in-out infinite;
}

.auth-active #funny-h1 {
  top: 25%;
  font-size: clamp(2rem, 5vw, 3.5rem);
  animation:
    floatTitleSmall 3s ease-in-out infinite,
    shimmer 3s ease-in-out infinite;
}

.title-hidden {
  opacity: 0;
  pointer-events: none;
  animation: fadeOutUp 0.6s cubic-bezier(0.55, 0.085, 0.68, 0.53) forwards;
}

@keyframes fadeOutUp {
  from {
    opacity: 1;
    transform: translate(-50%, -50%) scale(1);
  }
  to {
    opacity: 0;
    transform: translate(-50%, -50%) translateY(-50px) scale(0.8);
  }
}

@keyframes hintPulse {
  0%, 100% {
    opacity: 0.8;
    transform: translateX(-50%) scale(1);
  }
  50% {
    opacity: 1;
    transform: translateX(-50%) scale(1.05);
  }
}

@keyframes bounceHover {
  0%, 100% { transform: translate(-50%, -50%) scale(1); }
  50% { transform: translate(-50%, -50%) scale(1.1) rotate(5deg); }
}

@keyframes explode {
  0% { transform: translate(-50%, -50%) scale(1); }
  30% { transform: translate(-50%, -50%) scale(1.3) rotate(10deg); }
  60% { transform: translate(-50%, -50%) scale(0.9) rotate(-5deg); }
  100% { transform: translate(-50%, -50%) scale(1.05) rotate(0deg); }
}

@keyframes shimmerFast {
  0%, 100% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
}

@keyframes floatTitle {
  0%, 100% { transform: translate(-50%, -50%) translateY(0px); }
  50% { transform: translate(-50%, -50%) translateY(-20px); }
}

@keyframes rotateTitle {
  0%, 100% { transform: translate(-50%, -50%) rotate(0deg); }
  25% { transform: translate(-50%, -50%) rotate(2deg); }
  75% { transform: translate(-50%, -50%) rotate(-2deg); }
}

@keyframes shimmer {
  0%, 100% { background-position: 0% 50%; }
  50% { background-position: 100% 50%; }
}

.auth-active #funny-h1 {
  top: 15%;
  font-size: clamp(2rem, 5vw, 3.5rem);
  animation:
    floatTitleSmall 3s ease-in-out infinite,
    shimmer 3s ease-in-out infinite;
}

@keyframes floatTitleSmall {
  0%, 100% { transform: translate(-50%, -50%) translateY(0px); }
  50% { transform: translate(-50%, -50%) translateY(-10px); }
}

#auth-container {
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 5;
  background: var(--card-bg);
  backdrop-filter: blur(20px);
  border: 1px solid var(--card-border);
  border-radius: 25px;
  padding: clamp(1rem, 4vmin, 2rem);
  box-shadow: var(--shadow);
  min-width: min(350px, 95vw);
  max-width: 400px;
  max-height: calc(100vh - 2rem);
  overflow: hidden; 
  animation: slideUpAuthFixed 1.2s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  display: flex;
  flex-direction: column;
  justify-content: center;
}

@keyframes slideUpAuthFixed {
  from {
    opacity: 0;
    transform: translate(-50%, -50%) translateY(50px) scale(0.9);
  }
  to {
    opacity: 1;
    transform: translate(-50%, -50%) translateY(0px) scale(1);
  }
}

.login, .sign-up {
  color: var(--text-primary);
}

.login h2, .sign-up h2 {
  text-align: center;
  margin-bottom: 2rem;
  font-weight: 600;
  font-size: 1.8rem;
  background: linear-gradient(45deg, #fff, #e6f3ff);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

.input-group {
  margin-bottom: 1.5rem;
  position: relative;
}

.input-group label {
  display: block;
  margin-bottom: 0.5rem;
  font-weight: 500;
  color: var(--text-secondary);
  font-size: 0.9rem;
  text-transform: uppercase;
  letter-spacing: 0.5px;
}

.input-group input {
  width: 100%;
  padding: 1rem 1.2rem;
  border: 2px solid rgba(255, 255, 255, 0.1);
  border-radius: 15px;
  background: rgba(255, 255, 255, 0.05);
  color: var(--text-primary);
  font-size: 1rem;
  transition: all 0.3s ease;
  backdrop-filter: blur(10px);
}

.input-group input:focus {
  outline: none;
  border-color: rgba(255, 255, 255, 0.4);
  background: rgba(255, 255, 255, 0.1);
  box-shadow: 0 0 20px rgba(255, 255, 255, 0.1);
  transform: translateY(-2px);
}

.input-group input::placeholder {
  color: rgba(255, 255, 255, 0.5);
}

button {
  width: 100%;
  padding: 1rem 2rem;
  border: none;
  border-radius: 15px;
  font-size: 1rem;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 1px;
  cursor: pointer;
  transition: all 0.3s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  position: relative;
  margin-bottom: 1rem;
}

button[type="submit"] {
  background: var(--primary-gradient);
  color: white;
  box-shadow: 0 10px 30px rgba(102, 126, 234, 0.3);
}

button[type="submit"]:hover {
  transform: translateY(-3px);
  box-shadow: 0 15px 40px rgba(102, 126, 234, 0.4);
}

button[type="submit"]:active {
  transform: translateY(-1px);
}

button:not([type="submit"]) {
  background: rgba(255, 255, 255, 0.1);
  border: 2px solid rgba(255, 255, 255, 0.2);
  color: var(--text-primary);
  font-weight: 500;
  text-transform: none;
  letter-spacing: normal;
  margin-top: 1rem;
}

button:not([type="submit"]):hover {
  background: rgba(255, 255, 255, 0.2);
  border-color: rgba(255, 255, 255, 0.4);
  transform: translateY(-2px);
}

button::before {
  content: '';
  position: absolute;
  top: 50%;
  left: 50%;
  width: 0;
  height: 0;
  border-radius: 50%;
  background: rgba(255, 255, 255, 0.2);
  transition: all 0.6s ease;
  transform: translate(-50%, -50%);
  z-index: 0;
}

button:active::before {
  width: 300px;
  height: 300px;
}

button {
  overflow: hidden;
  position: relative;
}

button > * {
  position: relative;
  z-index: 1;
}

.loading-container {
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  gap: 20px;
  z-index: 20;
}

.spinner {
  width: 60px;
  height: 60px;
  border: 4px solid rgba(255, 255, 255, 0.1);
  border-top: 4px solid #ffffff;
  border-radius: 50%;
  animation: spin 1s linear infinite;
  box-shadow: 0 0 30px rgba(255, 255, 255, 0.3);
}

.loading-text {
  font-size: 18px;
  color: var(--text-primary);
  font-weight: 500;
  text-align: center;
  animation: pulse 2s ease-in-out infinite;
}

@keyframes pulse {
  0%, 100% { opacity: 1; }
  50% { opacity: 0.7; }
}

#logout-container {
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
  color: var(--text-primary);
  background: var(--card-bg);
  backdrop-filter: blur(20px);
  border: 1px solid var(--card-border);
  border-radius: 25px;
  padding: 3rem;
  box-shadow: var(--shadow);
  animation: fadeInContent 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275);
}

#logout-container h2 {
  font-size: 2.5rem;
  margin-bottom: 1rem;
  background: linear-gradient(45deg, #fff, #e6f3ff);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
}

#logout-container p {
  font-size: 1.2rem;
  margin-bottom: 2rem;
  color: var(--text-secondary);
}

.page-container {
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: clamp(0.5rem, 2vmin, 1rem);
  z-index: 5;
  overflow: hidden;
}

.content-card {
  background: var(--card-bg);
  backdrop-filter: blur(20px);
  border: 1px solid var(--card-border);
  border-radius: 25px;
  padding: clamp(1rem, 2.5vmin, 1.8rem);
  box-shadow: var(--shadow);
  text-align: center;
  color: var(--text-primary);
  max-width: min(92vw, 550px);
  width: 100%;
  animation: fadeInContent 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  max-height: calc(100vh - 4rem);
  height: fit-content;
  overflow: hidden; 
  position: relative;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

@keyframes fadeInContent {
  from {
    opacity: 0;
    transform: translateY(20px) scale(0.95);
  }
  to {
    opacity: 1;
    transform: translateY(0px) scale(1);
  }
}

.go-back-button {
  position: absolute !important;
  top: 1rem !important;
  left: 1rem !important;
  width: auto !important;
  padding: 0.6rem 1.2rem !important;
  background: rgba(255, 255, 255, 0.1) !important;
  border: 2px solid rgba(255, 255, 255, 0.2) !important;
  border-radius: 12px !important;
  color: var(--text-primary) !important;
  font-size: 0.85rem !important;
  font-weight: 500 !important;
  text-transform: none !important;
  letter-spacing: normal !important;
  margin: 0 !important;
  transition: all 0.3s ease !important;
  backdrop-filter: blur(10px) !important;
  z-index: 10 !important;
}

.go-back-button:hover {
  background: rgba(255, 255, 255, 0.2) !important;
  border-color: rgba(255, 255, 255, 0.4) !important;
  transform: translateX(-3px) !important;
}

.go-back-button:active {
  transform: translateX(-1px) !important;
}

.content-card .page-title {
  margin-top: 3.5rem;
}

.page-title {
  font-size: 2.5rem;
  margin-bottom: 0.8rem;
  background: linear-gradient(45deg, #fff, #e6f3ff, #d1ecf1);
  background-size: 200% 200%;
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
  animation: shimmer 4s ease-in-out infinite;
  font-weight: 700;
}

.page-description {
  font-size: 1.1rem;
  color: var(--text-secondary);
  margin-bottom: 2.5rem;
  line-height: 1.6;
}

.ranking-container .page-title {
  font-size: 2rem;
  margin-bottom: 0.4rem;
  margin-top: 3rem;
}

.ranking-container .page-description {
  font-size: 0.95rem;
  margin-bottom: 0.8rem;
}

.feature-preview {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(180px, 1fr));
  gap: 1.5rem;
  margin-bottom: 2rem;
}

.preview-item {
  background: rgba(255, 255, 255, 0.05);
  border-radius: 20px;
  padding: 2rem 1.5rem;
  border: 1px solid rgba(255, 255, 255, 0.1);
  transition: all 0.3s ease;
  animation: fadeInUp 0.6s ease-out;
}

.preview-item:nth-child(2) {
  animation-delay: 0.2s;
}

.preview-item:nth-child(3) {
  animation-delay: 0.4s;
}

.preview-item:hover {
  transform: translateY(-10px);
  background: rgba(255, 255, 255, 0.1);
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.1);
}

.preview-icon {
  font-size: 3rem;
  margin-bottom: 1rem;
  display: block;
}

.preview-item h3 {
  font-size: 1.3rem;
  margin-bottom: 0.5rem;
  color: var(--text-primary);
  font-weight: 600;
}

.preview-item p {
  color: var(--text-secondary);
  font-size: 0.95rem;
}

.leaderboard-preview {
  margin-bottom: 2rem;
}

.rank-item {
  display: flex;
  align-items: center;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 15px;
  padding: 1.5rem;
  margin-bottom: 1rem;
  border: 1px solid rgba(255, 255, 255, 0.1);
  transition: all 0.3s ease;
  animation: slideInLeft 0.6s ease-out;
}

.rank-item:nth-child(2) {
  animation-delay: 0.1s;
}

.rank-item:nth-child(3) {
  animation-delay: 0.2s;
}

.rank-item:nth-child(4) {
  animation-delay: 0.3s;
}

.rank-item:hover {
  transform: translateX(10px);
  background: rgba(255, 255, 255, 0.1);
}

.rank-gold {
  background: linear-gradient(45deg, rgba(255, 215, 0, 0.2), rgba(255, 255, 255, 0.1));
  border-color: rgba(255, 215, 0, 0.3);
}

.rank-silver {
  background: linear-gradient(45deg, rgba(192, 192, 192, 0.2), rgba(255, 255, 255, 0.1));
  border-color: rgba(192, 192, 192, 0.3);
}

.rank-bronze {
  background: linear-gradient(45deg, rgba(205, 127, 50, 0.2), rgba(255, 255, 255, 0.1));
  border-color: rgba(205, 127, 50, 0.3);
}

.rank-number {
  font-size: 2rem;
  font-weight: bold;
  margin-right: 2rem;
  min-width: 50px;
  color: var(--text-primary);
}

.rank-info {
  text-align: left;
  flex: 1;
}

.rank-info h3 {
  margin-bottom: 0.5rem;
  font-size: 1.2rem;
  color: var(--text-primary);
}

.rank-info p {
  color: var(--text-secondary);
  font-weight: 600;
}

.home-card {
  max-width: 500px;
}

.welcome-title {
  font-size: 2.5rem;
  margin-bottom: 2rem;
  background: linear-gradient(45deg, #fff, #e6f3ff);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
  font-weight: 700;
}

.user-info {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 3rem;
  padding: 1.5rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 20px;
  border: 1px solid rgba(255, 255, 255, 0.1);
}

.user-avatar {
  font-size: 3rem;
  margin-right: 1rem;
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  border-radius: 50%;
  width: 60px;
  height: 60px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.username {
  font-size: 1.5rem;
  font-weight: 600;
  color: var(--text-primary);
}

.navigation-grid {
  display: grid;
  grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
  gap: 1.5rem;
  margin-bottom: 3rem;
}

.nav-card {
  display: block;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 20px;
  padding: 2rem;
  border: 1px solid rgba(255, 255, 255, 0.1);
  text-decoration: none;
  color: var(--text-primary);
  transition: all 0.3s cubic-bezier(0.175, 0.885, 0.32, 1.275);
  position: relative;
  overflow: hidden;
}

.nav-card::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: var(--primary-gradient);
  opacity: 0;
  transition: opacity 0.3s ease;
}

.nav-card:hover::before {
  opacity: 0.1;
}

.nav-card:hover {
  transform: translateY(-10px) scale(1.05);
  box-shadow: 0 20px 40px rgba(0, 0, 0, 0.2);
  border-color: rgba(255, 255, 255, 0.3);
}

.nav-icon {
  font-size: 3rem;
  margin-bottom: 1rem;
  position: relative;
  z-index: 1;
}

.nav-card h3 {
  font-size: 1.3rem;
  margin-bottom: 0.5rem;
  font-weight: 600;
  position: relative;
  z-index: 1;
}

.nav-card p {
  color: var(--text-secondary);
  font-size: 0.95rem;
  position: relative;
  z-index: 1;
}

.start-button, .view-button {
  background: var(--success-gradient) !important;
  color: white !important;
  font-size: 1.2rem !important;
  padding: 1.2rem 2.5rem !important;
  margin: 1rem 0 !important;
  box-shadow: 0 15px 35px rgba(67, 233, 123, 0.3) !important;
  animation: pulse 2s infinite;
}

.start-button:hover, .view-button:hover {
  transform: translateY(-5px) scale(1.05) !important;
  box-shadow: 0 20px 45px rgba(67, 233, 123, 0.4) !important;
}

.logout-button {
  background: rgba(255, 255, 255, 0.1) !important;
  border: 2px solid rgba(255, 255, 255, 0.2) !important;
  color: var(--text-primary) !important;
  font-weight: 500 !important;
}

.logout-button:hover {
  background: rgba(255, 85, 108, 0.2) !important;
  border-color: rgba(255, 85, 108, 0.4) !important;
}

.coming-soon {
  font-style: italic;
  color: var(--text-secondary);
  font-size: 0.9rem;
}

@keyframes fadeInUp {
  from {
    opacity: 0;
    transform: translateY(30px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

@keyframes slideInLeft {
  from {
    opacity: 0;
    transform: translateX(-30px);
  }
  to {
    opacity: 1;
    transform: translateX(0);
  }
}

@media (max-height: 700px) {
  #auth-container {
    padding: 1rem;
    max-height: 100vh;
  }

  .content-card {
    padding: 1rem;
    max-height: 100vh;
  }

  .page-title {
    font-size: 2rem !important;
    margin-bottom: 0.5rem !important;
  }

  .page-description {
    font-size: 1rem !important;
    margin-bottom: 1rem !important;
  }

  .feature-preview,
  .leaderboard-preview {
