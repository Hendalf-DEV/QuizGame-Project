.quiz-active {
  max-width: min(92vw, 700px) !important;
  max-height: calc(100vh - 2rem) !important;
  overflow: hidden !important;
  justify-content: flex-start !important;
  padding: clamp(0.8rem, 2vmin, 1.5rem) !important;
}

.quiz-header {
  margin-bottom: 1.2rem;
  margin-top: 2.5rem;
  flex-shrink: 0;
}

.quiz-info {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 0.8rem;
  padding: 0 0.5rem;
}

.question-counter,
.score-counter {
  font-size: 0.9rem;
  font-weight: 600;
  color: var(--text-primary);
  background: rgba(255, 255, 255, 0.1);
  padding: 0.4rem 0.8rem;
  border-radius: 12px;
  border: 1px solid rgba(255, 255, 255, 0.2);
}

.score-counter {
  background: linear-gradient(135deg, rgba(67, 233, 123, 0.2), rgba(56, 249, 215, 0.2));
  border-color: rgba(67, 233, 123, 0.3);
}

.progress-bar {
  width: 100%;
  height: 8px;
  background: rgba(255, 255, 255, 0.1);
  border-radius: 10px;
  overflow: hidden;
  position: relative;
}

.progress-fill {
  height: 100%;
  background: var(--warning-gradient);
  border-radius: 10px;
  transition: width 0.5s cubic-bezier(0.4, 0, 0.2, 1);
  box-shadow: 0 0 10px rgba(67, 233, 123, 0.5);
}

.quiz-question {
  margin-bottom: 1.2rem;
  padding: 1.2rem;
  background: rgba(255, 255, 255, 0.05);
  border-radius: 20px;
  border: 1px solid rgba(255, 255, 255, 0.1);
  flex-shrink: 0;
}

.question-text {
  font-size: 1.2rem;
  color: var(--text-primary);
  font-weight: 600;
  line-height: 1.5;
  margin: 0;
}

.quiz-options {
  display: flex;
  flex-direction: column;
  gap: 0.8rem;
  margin-bottom: 1rem;
  flex-shrink: 0;
}

.quiz-option {
  display: flex;
  align-items: center;
  gap: 0.8rem;
  padding: 0.9rem 1.2rem;
  background: rgba(255, 255, 255, 0.05);
  border: 2px solid rgba(255, 255, 255, 0.1);
  border-radius: 15px;
  color: var(--text-primary);
  font-size: 0.95rem;
  font-weight: 500;
  text-align: left;
  transition: all 0.3s ease;
  cursor: pointer;
  position: relative;
  overflow: hidden;
}

.quiz-option::before {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 0;
  height: 100%;
  background: rgba(255, 255, 255, 0.1);
  transition: width 0.3s ease;
  z-index: 0;
}

.quiz-option:hover:not(:disabled)::before {
  width: 100%;
}

.quiz-option:hover:not(:disabled) {
  border-color: rgba(255, 255, 255, 0.3);
  transform: translateX(5px);
}

.quiz-option:active:not(:disabled) {
  transform: translateX(3px) scale(0.98);
}

.quiz-option.selected {
  background: rgba(102, 126, 234, 0.2);
  border-color: rgba(102, 126, 234, 0.5);
}

.quiz-option.correct {
  background: linear-gradient(135deg, rgba(67, 233, 123, 0.2), rgba(56, 249, 215, 0.2));
  border-color: rgba(67, 233, 123, 0.6);
  animation: correctPulse 0.6s ease-out;
}

.quiz-option.incorrect {
  background: linear-gradient(135deg, rgba(240, 147, 251, 0.2), rgba(245, 87, 108, 0.2));
  border-color: rgba(245, 87, 108, 0.6);
  animation: incorrectShake 0.6s ease-out;
}

.quiz-option.disabled {
  opacity: 0.5;
  cursor: not-allowed;
}

@keyframes correctPulse {
  0%, 100% { transform: scale(1); }
  50% { transform: scale(1.05); }
}

@keyframes incorrectShake {
  0%, 100% { transform: translateX(0); }
  25% { transform: translateX(-10px); }
  75% { transform: translateX(10px); }
}

.option-letter {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 32px;
  height: 32px;
  background: rgba(255, 255, 255, 0.1);
  border-radius: 50%;
  font-weight: 700;
  font-size: 0.9rem;
  flex-shrink: 0;
  z-index: 1;
}

.quiz-option.correct .option-letter {
  background: rgba(67, 233, 123, 0.3);
}

.quiz-option.incorrect .option-letter {
  background: rgba(245, 87, 108, 0.3);
}

.option-text {
  flex: 1;
  z-index: 1;
}

.option-icon {
  font-size: 1.5rem;
  font-weight: bold;
  z-index: 1;
}

.quiz-option.correct .option-icon {
  color: #43e97b;
}

.quiz-option.incorrect .option-icon {
  color: #f5576c;
}

.next-button {
  width: 100%;
  padding: 0.8rem 1.5rem;
  background: var(--primary-gradient);
  border: 2px solid rgba(255, 255, 255, 0.2);
  border-radius: 15px;
  color: var(--text-primary);
  font-size: 1rem;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 1px;
  cursor: pointer;
  transition: all 0.3s ease;
  box-shadow: 0 10px 30px rgba(102, 126, 234, 0.3);
  animation: slideInUp 0.5s ease-out;
  margin-top: 0.5rem;
  flex-shrink: 0;
}

.next-button:hover {
  transform: translateY(-3px);
  box-shadow: 0 15px 40px rgba(102, 126, 234, 0.4);
}

.next-button:active {
  transform: translateY(-1px);
}

@keyframes slideInUp {
  from {
    opacity: 0;
    transform: translateY(20px);
  }
  to {
    opacity: 1;
    transform: translateY(0);
  }
}

.quiz-complete {
  max-width: min(92vw, 600px) !important;
}

.quiz-result {
  padding: 2rem 1rem;
}

.result-title {
  font-size: 2.5rem;
  margin-bottom: 2rem;
  margin-top: 3rem;
  background: linear-gradient(45deg, #fff, #e6f3ff);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
  background-clip: text;
  font-weight: 700;
  animation: fadeInScale 0.8s ease-out;
}

@keyframes fadeInScale {
  from {
    opacity: 0;
    transform: scale(0.8);
  }
  to {
    opacity: 1;
    transform: scale(1);
  }
}

.score-display {
  display: flex;
  flex-direction: column;
  align-items: center;
  gap: 1.5rem;
  margin-bottom: 2rem;
  animation: fadeInUp 1s ease-out;
}

.score-circle {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 180px;
  height: 180px;
  background: var(--primary-gradient);
  border-radius: 50%;
  border: 5px solid rgba(255, 255, 255, 0.3);
  box-shadow: 0 20px 60px rgba(102, 126, 234, 0.4);
  position: relative;
}

.score-circle::before {
  content: '';
  position: absolute;
  top: -10px;
  left: -10px;
  right: -10px;
  bottom: -10px;
  border-radius: 50%;
  background: linear-gradient(45deg, transparent, rgba(255, 255, 255, 0.1));
  animation: rotate 3s linear infinite;
  z-index: -1;
}

@keyframes rotate {
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }
}

.score-number {
  font-size: 4rem;
  font-weight: 700;
  color: var(--text-primary);
  line-height: 1;
}

.score-total {
  font-size: 1.5rem;
  color: rgba(255, 255, 255, 0.8);
}

.percentage {
  font-size: 2.5rem;
  font-weight: 700;
  color: var(--text-primary);
  background: rgba(255, 255, 255, 0.1);
  padding: 0.5rem 2rem;
  border-radius: 20px;
  border: 2px solid rgba(255, 255, 255, 0.2);
}

.result-message {
  font-size: 1.3rem;
  color: var(--text-secondary);
  margin-bottom: 2.5rem;
  animation: fadeIn 1.2s ease-out;
}

@keyframes fadeIn {
  from { opacity: 0; }
  to { opacity: 1; }
}

.result-actions {
  display: flex;
  flex-direction: column;
  gap: 1rem;
  width: 100%;
}

.primary-button,
.secondary-button {
  width: 100%;
  padding: 0.75rem 1.5rem;
  border-radius: 12px;
  font-size: 1rem;
  font-weight: 600;
  cursor: pointer;
  transition: all 0.3s ease;
  border: 2px solid rgba(255, 255, 255, 0.2);
  text-transform: none;
  letter-spacing: normal;
}

.primary-button {
  background: var(--primary-gradient);
  color: var(--text-primary);
}

.primary-button:hover {
  transform: translateY(-3px);
}

.secondary-button {
  background: rgba(255, 255, 255, 0.05);
  color: var(--text-primary);
  border-color: rgba(255, 255, 255, 0.2);
}

.secondary-button:hover {
  background: rgba(255, 255, 255, 0.1);
  border-color: rgba(255, 255, 255, 0.3);
  transform: translateY(-2px);
}

.primary-button:active,
.secondary-button:active {
  transform: translateY(0);
}

